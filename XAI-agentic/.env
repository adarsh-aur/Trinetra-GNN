# Groq API Configuration (REQUIRED)
GROQ_API_KEY=YOUR_GROQ_API_KEY_HERE
GROQ_MODEL=openai/gpt-oss-20b
GROQ_MAX_TOKENS=8000
GROQ_TEMPERATURE=0.7

# Get free key from: https://console.groq.com/keys

# NVD API Configuration (OPTIONAL but recommended for faster CVE lookups)
# Get free key from: https://nvd.nist.gov/developers/request-an-api-key
ANALYZE_CVES=true
NVD_ENABLED=true
NVD_API_KEY=b924563d-868f-4022-900e-deff8c57af60
NVD_CACHE_TIMEOUT=24

# Graph Neural Network Configuration
ZSCORE_THRESHOLD=3.0
BATCH_SIZE=32
UPDATE_INTERVAL=2.0
LOG_LEVEL=INFO
ANALYZE_CVES=true


# Agent Configuration
AGENT_NAME=GNN-Guardian
AGENT_ROLE=Network Security Analysis


# CVE Analysis Configuration
ANALYZE_CVES=true
MAX_CVES_PER_NODE=10
# CVE_SEVERITY_FILTER=CRITICAL,HIGH


# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/agent.log
